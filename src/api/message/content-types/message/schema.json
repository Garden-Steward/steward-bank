      "enum": [
        "question",
        "followup",
        "reply",
        "notification",
        "complete"
      ]
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "garden": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::garden.garden"
    },
    "garden_task": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::garden-task.garden-task"
    }
  }
}
