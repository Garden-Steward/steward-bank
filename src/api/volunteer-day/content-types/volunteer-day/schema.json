{
  "kind": "collectionType",
  "collectionName": "volunteer_days",
  "info": {
    "singularName": "volunteer-day",
    "pluralName": "volunteer-days",
    "displayName": "Volunteer Day",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "blurb": {
      "type": "text"
    },
    "garden": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::garden.garden"
    },
    "content": {
      "type": "richtext"
    },
    "startDatetime": {
      "type": "datetime"
    },
    "endText": {
      "type": "string"
    },
    "disabled": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "garden_tasks": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::garden-task.garden-task",
      "mappedBy": "volunteer_day"
    },
    "interest": {
      "type": "string",
      "default": "Everyone"
    },
    "sms_campaigns": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::sms-campaign.sms-campaign",
      "mappedBy": "volunteer_day"
    },
    "slug": {
      "type": "string"
    },
    "confirmed": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user"
    },
    "accessibility": {
      "type": "enumeration",
      "enum": [
        "Public",
        "Garden Members",
        "Invite Only"
      ],
      "default": "Public"
    },
    "planting": {
      "displayName": "planting",
      "type": "component",
      "repeatable": true,
      "component": "plants.planting"
    },
    "messages": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::message.message",
      "mappedBy": "event"
    },
    "smsLink": {
      "type": "boolean",
      "default": false
    },
    "hero_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "general",
        "land_work",
        "cleanup",
        "art",
        "workshop"
      ]
    },
    "partiful_link": {
      "type": "string"
    },
    "featured_gallery": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": ["images"],
      "description": "3-8 favorite photos from the event"
    },
    "photo_album_url": {
      "type": "string",
      "description": "Link to Google Photos shared album"
    },
    "photo_album_id": {
      "type": "string",
      "description": "Extracted Google Photos album ID"
    }
  }
}
