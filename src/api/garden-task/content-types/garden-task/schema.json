{
  "kind": "collectionType",
  "collectionName": "garden_tasks",
  "info": {
    "singularName": "garden-task",
    "pluralName": "garden-tasks",
    "displayName": "Garden Task",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "INITIALIZED",
        "PENDING",
        "INTERESTED",
        "STARTED",
        "FINISHED",
        "ABANDONED",
        "ISSUE",
        "SKIPPED",
        "RESOLVED"
      ]
    },
    "overview": {
      "type": "text"
    },
    "started_at": {
      "type": "datetime"
    },
    "completed_at": {
      "type": "datetime"
    },
    "recurring_task": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::recurring-task.recurring-task"
    },
    "garden": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::garden.garden"
    },
    "volunteers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "General",
        "Water",
        "Weeding",
        "Planting",
        "Harvest"
      ]
    },
    "volunteer_day": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::volunteer-day.volunteer-day",
      "inversedBy": "garden_tasks"
    },
    "max_volunteers": {
      "type": "integer"
    },
    "primary_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "complete_once": {
      "type": "boolean",
      "default": true
    },
    "current_section": {
      "type": "richtext",
      "description": "Current task - what is expected of the volunteer"
    },
    "tools_section": {
      "type": "richtext",
      "description": "Tools and supplies needed for this task"
    },
    "resources_section": {
      "type": "richtext",
      "description": "Resources including optional video link"
    }
  }
}
