{
  "kind": "collectionType",
  "collectionName": "recurring_tasks",
  "info": {
    "singularName": "recurring-task",
    "pluralName": "recurring-tasks",
    "displayName": "Recurring Task",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "overview": {
      "type": "text"
    },
    "garden": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::garden.garden",
      "inversedBy": "recurring_tasks"
    },
    "complete_once": {
      "type": "boolean",
      "default": true
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "General",
        "Water",
        "Weeding",
        "Planting",
        "Harvest"
      ]
    },
    "schedulers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::scheduler.scheduler",
      "mappedBy": "recurring_task"
    },
    "scheduler_type": {
      "type": "enumeration",
      "enum": [
        "No Schedule",
        "Daily Primary",
        "Weekly Shuffle"
      ],
      "required": true,
      "default": "No Schedule"
    },
    "instruction": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::instruction.instruction"
    },
    "max_volunteers": {
      "type": "integer"
    },
    "primary_image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    }
  }
}
